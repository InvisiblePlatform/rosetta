{{- $tempDict := (dict "k" .Params.tags) -}}
{{- with .Params.tag_data -}}
{{- $tempDict = merge $tempDict . -}}
{{- end -}}
{{- with .Params.mbfc_source -}}
{{- $tempDict = merge $tempDict (dict "_m" . ) -}}
{{- end -}}
{{- with .Params.mbfc_tags -}}
{{- $tempDict = merge $tempDict (dict "m" . ) -}}
{{- end -}}
{{- $.Scratch.SetInMap "index" "data" $tempDict -}}
{{- with .Params.connections -}}
{{- $.Scratch.SetInMap "index" "connections" . -}}
{{- end -}}
{{- with .Params.social -}}
{{- $.Scratch.SetInMap "index" "social" . -}}
{{- end -}}
{{- with .Params.political -}}
{{- $.Scratch.SetInMap "index" "political" . -}}
{{- end -}}
{{- with .Params.core -}}
{{- $.Scratch.SetInMap "index" "core" . -}}
{{- end -}}
{{- with .Params.title -}}
{{- $.Scratch.SetInMap "index" "title" . -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
