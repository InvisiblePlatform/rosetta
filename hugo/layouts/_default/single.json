{{- $tempDict := (dict "k" .Params.tags) -}}
{{- with .Params.bcorp_rating -}}
{{- $tempDict = merge $tempDict (dict "b" . ) -}}
{{- end -}}
{{- with .Params.glassdoor_rating -}}
{{- $tempDict = merge $tempDict (dict "l" . ) -}}
{{- end -}}
{{- with .Params.tosdr_rating -}}
{{- $tempDict = merge $tempDict (dict "P" . ) -}}
{{- end -}}
{{- with .Params.ts_rating -}}
{{- $tempDict = merge $tempDict (dict "s"  . ) -}}
{{- end -}}
{{- with .Params.tp_rating -}}
{{- $tempDict = merge $tempDict (dict "t" . ) -}}
{{- end -}}
{{- with .Params.mbfc_tags -}}
{{- $tempDict = merge $tempDict (dict "m" . ) -}}
{{- end -}}
{{- $.Scratch.SetInMap "index" "data" $tempDict -}}
{{- with .Params.connections -}}
{{- $.Scratch.SetInMap "index" "connections" . -}}
{{- end -}}
{{- with .Params.core -}}
{{- $.Scratch.SetInMap "index" "core" . -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
