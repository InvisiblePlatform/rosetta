{{- $.Scratch.Add "index" slice -}}
{{- range where .Site.Pages "Type" "not in"  (slice "page" "json") -}}
    {{- $tempslice := (slice ) -}}
    {{-  range $web := .Params.website -}}{{- $value := . | replaceRE "^https?://(www.)?(.*)" "$2" -}}{{- $tempslice = $tempslice | append $value -}}{{- end -}}
        {{- if ne (index $tempslice 0 ) "" -}}
        {{- $.Scratch.Add "index" (dict "id" (lower .Params.wikidataid) "websites" $tempslice "lastupdated" .Params.updated ) -}}
    {{- end -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
